<p style="color: green"><%= notice %></p>

<div id="agreement_contracts">
  <% @agreement_contracts.each do |agreement_contract| %>
    <strong>Cooperative:</strong> <%= agreement_contract.cooperative&.name %><br>
    <strong>Insurance Product:</strong> <%= agreement_contract.insurance_product&.name %><br>

  <% end %>

<div>
  <%= link_to "Edit this contract", edit_agreement_contract_path(@agreement_contract) %> |
  <%= link_to "Back to contracts", agreement_contracts_path %>

  <%= button_to "Destroy this contract", @agreement_contract, method: :delete %>
</div>
